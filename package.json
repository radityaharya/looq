{
	"name": "looq",
	"description": "A SearXNG frontend",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"dev": "dev:vite",
		"dev:wrangler": "if [ -d ./dist ]; then echo 'serving frontend from ./dist'; else echo 'dist folder not found'; fi && wrangler pages dev --local",
		"dev:vite": "dotenv -e .dev.vars vite",
		"start": "dotenv -e .prod.vars bun build/server.js",
		"build": "bun run typecheck && vite build && rollup -c rollup.config.mjs",
		"build:bun": "NODE_ENV=production && vite build && bun build --target bun --compile --smol --minify --sourcemap --outfile server server.ts",
		"deploy": "wrangler pages deploy",
		"db:generate": "dotenv -e .dev.vars drizzle-kit generate",
		"db:up": "dotenv -e .dev.vars drizzle-kit up",
		"db:migrate": "dotenv -e .dev.vars drizzle-kit migrate",
		"db:push": "dotenv -e .dev.vars drizzle-kit push",
		"db:studio:wrangler": "dotenv -e .dev.vars RUNTIME=workerd drizzle-kit studio",
		"db:studio": "dotenv -e .dev.vars drizzle-kit studio",
		"typecheck": "tsc --noEmit",
		"typecheck:watch": "tsc --noEmit --watch",
		"lint": "biome check",
		"i18next-resources-for-ts": "i18next-resources-for-ts toc -i ./src/i18n/locales/en -o ./src/types/resources.ts",
		"translate": "transmart"
	},
